{
  "name": "@onem/ts-example",
  "namespace": "onem",
  "private": true,
  "repository": "https://github.com/jayvazmm/onem-ts-example",
  "scripts": {
    "format": "npx lerna run format",
    "build": "npx lerna run build",
    "esbuild": "npm run build && npx lerna run esbuild",
    "codegen": "graphql-codegen",
    "test": "npx lerna run test -- ",
    "test:integ": "npx lerna run test:integ --",
    "test:ci": "npx lerna run test:ci",
    "clean": "npx lerna clean --yes && rm -rf node_modules coverage cdk.out && npx lerna run clean",
    "cdk": "npx lerna run cdk:lambda --scope @onem/cdk -- ",
    "cdk:lambda": "npx lerna run cdk:lambda --scope @onem/cdk -- ",
    "cdk:ecs": "npx lerna run cdk:ecs --scope @onem/cdk -- ",
    "lint": "eslint 'packages/**/*.{js,ts}'",
    "lint:ci": "npx lerna run lint:ci",
    "lint:fix": "npx lerna run lint:fix",
    "prettier": "npx lerna run prettier",
    "prettier:ci": "npx lerna run prettier:ci",
    "prettier:fix": "npx lerna run prettier:fix",
    "start": "npx lerna run build && npx lerna run start",
    "versions": "node --version && npm --version && npx lerna --version"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/graphql-modules-preset": "^4.0.1",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.9.1",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "aws-cdk": "2.110.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "lerna": "^7.4.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "workspaces": [
    "packages/lib-ts-service",
    "packages/cdk",
    "packages/aws-ecs-service",
    "packages/aws-lambda-service"
  ]
}
